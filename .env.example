# API Credentials
REACT_APP_PINGTREE_TOKEN=your_pingtree_token_here
REACT_APP_PARTNER_ID=your_partner_id_here
REACT_APP_SUBSCRIPTION_KEY=your_subscription_key_here
REACT_APP_CREATIVE_ID=your_creative_id_here

# Configuration
REACT_APP_API_TEST_MODE=true

# Pingtree Development/Testing settings
# This ensures form submissions always succeed for testing/demo purposes
# Set to false in production when ready to make real API calls
REACT_APP_SIMULATE_API=false

# API endpoint for submissions
REACT_APP_API_BASE_URL=https://api.example.com
NEXT_PUBLIC_API_BASE_URL=https://api.example.com

# Feature flags
REACT_APP_ENABLE_GEOLOCATION=true

# API Keys and Endpoints
REACT_APP_ANALYTICS_ENDPOINT=your_analytics_endpoint_here

# Lead storage API endpoint
REACT_APP_LEAD_API_ENDPOINT=your_lead_api_endpoint_here 